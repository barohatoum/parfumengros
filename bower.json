{
  "name": "parfumengros",
  "version": "2",
  "authors": [
    "Ibrahim Hatoum <ibrahim.hatoum@gmail.com>"
  ],
  "description": "E-commerce website for perfume store",
  "main": "index.php",
  "keywords": [
    "perfume"
  ],
  "license": "MIT",
  "homepage": "http://parfumengros.ca",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "uikit": "~2.21.0",
    "jquery": "~2.1.4"
  },
  "install": {
  	"sources": {
  		"jquery": {
  			"mapping": [
  				{"bower_components/jquery/dist/jquery.min.js": "assets/js/vendor/jquery.min.js"},
  				{"bower_components/jquery/dist/jquery.min.map": "assets/js/vendor/jquery.min.map"}
  			]
  		},
  		"uikit": {
  			"mapping": [
  				{"bower_components/uikit/js/uikit.min.js": "assets/js/vendor/uikit.min.js"},
  				{"bower_components/uikit/fonts/fontawesome-webfont.eot": "assets/fonts/fontawesome-webfont.eot"},
  				{"bower_components/uikit/fonts/fontawesome-webfont.ttf": "assets/fonts/fontawesome-webfont.ttf"},
  				{"bower_components/uikit/fonts/fontawesome-webfont.woff": "assets/fonts/fontawesome-webfont.woff"},
  				{"bower_components/uikit/fonts/fontawesome-webfont.woff2": "assets/fonts/fontawesome-webfont.eot"},
  				{"bower_components/uikit/fonts/FontAwesome.otf": "assets/fonts/FontAwesome.otf"}
  			]
  		}
  	}
  }
}
